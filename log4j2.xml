<Configuration>
   <Appenders>
      <Console name="Console" target="SYSTEM_OUT">
         <PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level %logger{36} %M - %msg%n"/>
      </Console>
   </Appenders>
   <Loggers>
      <Root level="error">
         <AppenderRef ref="Console"/>
      </Root>
   </Loggers>
</Configuration>
